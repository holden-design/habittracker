# Database (will be set by Railway)
DATABASE_URL=postgresql://user:password@localhost/personalsystems

# Environment
NODE_ENV=development

# Server port
PORT=5000

# JWT Secret (change this in production!)
JWT_SECRET=your-secret-key-here

# Google OAuth (get at https://console.cloud.google.com/apis/credentials)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Facebook OAuth (get at https://developers.facebook.com/apps)
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id

# Claude AI (get your key at https://console.anthropic.com)
ANTHROPIC_API_KEY=your-api-key-here
